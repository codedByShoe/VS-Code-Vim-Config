{
  // General Settings
  "breadcrumbs.enabled": false,
  "css.lint.unknownAtRules": "ignore",
  "editor.occurrencesHighlight": "off",
  "editor.matchBrackets": "never",
  "editor.selectionHighlight": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabCompletion": "on",
  "editor.formatOnSave": true,
  "editor.stickyScroll.maxLineCount": 1,
  "editor.fontFamily": "CaskaydiaMono Nerd Font",
  "editor.tabSize": 2,
  "editor.stickyScroll.enabled": false,
  "editor.linkedEditing": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "auto",
  "editor.rulers": [
    100
  ],
  "editor.fontSize": 18,
  "editor.lineNumbers": "relative",
  "editor.lineHeight": 1.9,
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.cursorSurroundingLines": 20,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.openEditors.visible": 3,
  "explorer.decorations.colors": false,
  "extensions.ignoreRecommendations": true,
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "never",
  "glassit.alpha": 255,
  "git.enableSmartCommit": true,
  "git.ignoreMissingGitWarning": true,
  "git.confirmSync": false,
  "makefile.configureOnOpen": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.external.linuxExec": "/usr/bin/zsh",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font'",
  "workbench.colorTheme": "Kanagawa Wave",
  "workbench.settings.editor": "json",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.statusBar.visible": true,
  "workbench.sideBar.location": "right",
  "workbench.editor.showTabs": "none",
  "workbench.externalBrowser": "/usr/bin/brave",
  "workbench.activityBar.location": "hidden",
  "workbench.layoutControl.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.startupEditor": "none",
  "window.title": "${activeEditorShort}",
  "window.commandCenter": false,
  "window.menuBarVisibility": "toggle",
  "zenMode.showTabs": "none",
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "javascript.format.semicolons": "insert",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.tsdk": "node_modules/typescript/lib",
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.templ": "${capture}_templ.go,${capture}_templ.txt",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "vscode_custom_css.imports": [
    "file:///home/codedbyshoe/.config/vscode-powerline/powerline.css",
    "file:///home/codedbyshoe/.config/vscode-powerline/custom.css"
  ],
  "settingsSync.ignoredExtensions": [
    "ms-mssql.sql-bindings-vscode",
    "ms-mssql.data-workspace-vscode",
    "asvetliakov.vscode-neovim",
    "ms-mssql.mssql",
    "visualstudioexptteam.intellicode-api-usage-examples",
    "eliverlara.sweet-vscode-icons"
  ],
  // Language Specific settings
  "emmet.includeLanguages": {
    "twig": "html",
    "templ": "html"
  },
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.html": "html"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[handlebars]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Vim stuff
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        ";",
        ";"
      ],
      "after": [
        "<Esc>",
        "A",
        ";",
        "<Esc>"
      ]
    },
    {
      "before": [
        ",",
        ","
      ],
      "after": [
        "<Esc>",
        "A",
        ",",
        "<Esc>"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-p>": false,
    "<C-f>": false,
    "<C-z>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<tab>"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<S-tab>"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "1"
      ],
      "commands": [
        "vscode-harpoon.gotoEditor1"
      ]
    },
    {
      "before": [
        "<leader>",
        "2"
      ],
      "commands": [
        "vscode-harpoon.gotoEditor2"
      ]
    },
    {
      "before": [
        "<leader>",
        "3"
      ],
      "commands": [
        "vscode-harpoon.gotoEditor3"
      ]
    },
    {
      "before": [
        "<leader>",
        "4"
      ],
      "commands": [
        "vscode-harpoon.gotoEditor4"
      ]
    },
    {
      "before": [
        "<leader>",
        "n",
        "i"
      ],
      "commands": [
        "workbench.view.extension.nugetGalleryView"
      ]
    },
    {
      "before": [
        "<leader>",
        "h",
        "s"
      ],
      "commands": [
        "vscode-harpoon.editorQuickPick"
      ]
    },
    {
      "before": [
        "<leader>",
        "h",
        "a"
      ],
      "commands": [
        "vscode-harpoon.addEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "h",
        "e"
      ],
      "commands": [
        "vscode-harpoon.editEditors"
      ]
    },
    {
      "before": [
        "<leader>",
        "d",
        "r"
      ],
      "commands": [
        "workbench.action.debug.start"
      ]
    },
    {
      "before": [
        "<leader>",
        "d",
        "q"
      ],
      "commands": [
        "workbench.action.debug.stop"
      ]
    },
    {
      "before": [
        "<leader>",
        "b",
        "c"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "f"
      ],
      "commands": [
        "find-it-faster.findFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "b"
      ],
      "commands": [
        "find-it-faster.findWithinFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "/"
      ],
      "commands": [
        "editor.action.commentLine"
      ],
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showDefinitionPreviewHover"
      ]
    },
    {
      "before": [
        "<leader>",
        "z",
        "m"
      ],
      "commands": [
        "editor.fold"
      ]
    },
    {
      "before": [
        "<leader>",
        "z",
        "r"
      ],
      "commands": [
        "editor.unfold"
      ]
    },
    {
      "before": [
        "<leader>",
        "n",
        "f"
      ],
      "commands": [
        "extension.advancedNewFile"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "before": [
        "<leader>",
        "/"
      ],
      "commands": [
        "editor.action.commentLine"
      ],
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "before": [
        "p"
      ],
      "after": [
        "\"",
        "_",
        "d",
        "P"
      ]
    }
  ]
}
